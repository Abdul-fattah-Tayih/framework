{
    "name": "vanilo/framework",
    "description": "E-commerce Framework for Laravel",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "support": {
        "issues": "https://github.com/vanilophp/framework/issues"
    },
    "require": {
        "php": "^7.1.3",
        "konekt/appshell": "~1.2",
        "spatie/laravel-medialibrary": "^7.3",
        "vanilo/contracts": "0.5.*",
        "vanilo/support": "0.5.*",
        "vanilo/category": "0.5.*",
        "vanilo/product": "0.5.*",
        "vanilo/cart": "0.5.*",
        "vanilo/checkout": "0.5.*",
        "vanilo/order": "0.5.*"
    },
    "autoload": {
        "psr-4": {
            "Vanilo\\Framework\\": "src"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "require-dev": {
        "monolog/monolog": "^1.22",
        "orchestra/testbench": "^3.4.2",
        "phpunit/phpunit" : "6.2 - 7.4",
        "mockery/mockery": "^1.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Vanilo\\Framework\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5-dev"
        }
    }
}
